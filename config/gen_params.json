{	"setup":{
		"autopilot": {},
		"gazebo": {
			"gazebo_models": {
				"gazebo_model_repo_0":{
					"repo": "git@github.com:pr-db/robocon_gazebo.git",
					"version": "master",
					"name": "robocon_gazebo"
				}
			},
			"gazebo_plugins": {}
		},
		"ros2": {
			"ros2_repo_0":{
				"repo": "git@github.com:pr-db/robocon_interfaces.git",
				"version": "master",
				"build_package": "robocon_interfaces",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_1":{
				"repo": "git@github.com:pr-db/robocon_vision.git",
				"version": "master",
				"build_package": "robocon_vision",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_2":{
				"repo": "git@github.com:pr-db/track_follow.git",
				"version": "master",
				"build_package": "track_follow",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			}
		},
		"system": {
			"set_environment": {
				"environment_0": {
					"variable": "GAZEBO_MODEL_DATABASE_URI",
					"value": "",
					"method": "overwrite"
				},
				"environment_1": {
					"variable": "GAZEBO_RESOURCE_PATH",
					"value": "/usr/share/gazebo-11",
					"method": "prepend"
				},
				"environment_2": {
					"variable": "GAZEBO_PLUGIN_PATH",
					"value": "/opt/ros/foxy/lib",
					"method": "prepend"
				}
			}
		}
	},
	"verbose": {
		"gzclient": true,
		"gzserver": true
	},
	"nodes": {
		"ros2_node_0":{
			"timing": "post-spawn",
			"package": "robocon_vision",
			"executable": "robocon_vision",
			"name": "robocon_vision_0",
			"output": "screen",
			"parameters": [	{"pyramid_down": 3},
							{"camera_image": "trackImage0"},
							{"debug_image": "debugImage0"},
							{"namespace": "cupcar0"},
							{"mask_ratio_array": [1.0, 0.4]}

						]
		},
		"ros2_node_1":{
			"timing": "post-spawn",
			"package": "rqt_image_view",
			"executable": "rqt_image_view",
			"name": "rqt_image_view_0",
			"output": "screen",
			"parameters": []
		},
		"ros2_node_2":{
			"timing": "post-spawn",
			"package": "track_follow",
			"executable": "line_follower",
			"name": "line_follower_0",
			"output": "screen",
			"parameters": []
		}
	},
	"world_params": {
		"gazebo_name": "robocon_gazebo",
		"generate_world": 1,
		"generate_params":{
			"world_name": "canvas",
			"sdf_version": "1.6",
			"use_spherical_coords": 1,
			"lat_lon_alt": [28.6139, -77.2090, 100],
			"sun_model": "sunUTC",
			"sun_utc_date": "now",
			"ambient_light": 0.5,
			"background_light": 0.15,
			"shadows": 0,
			"cloud_speed": 12,
			"wind_speed": "NotSet",
			"embedded_models": {
				"embed_model_0": {
					"model": "robocon_raceway_overpass",
					"name": "robocon_raceway_overpass_0",
					"pose": "0 0 -0.009 0 0 0"
				},
				"embed_model_1": {
					"model": "start_point",
					"name": "start_point_0",
					"pose": "0.2 -0.45 0.09 0 0 0"
				},
				"embed_model_2": {
					"model": "start_point",
					"name": "start_point_1",
					"pose": "0.2 0.45 0.09 0 0 0"
				},
				"embed_model_3": {
					"model": "ground_plane",
					"name": "ground_plane",
					"pose": "0.0 0.0 0.0 0 0 0"
					}
				
			},
			"video_widget": "NotSet",
			"set_physics": 0,
			"update_rate": 100,
			"realtime_factor": 1.0,
			"ode_threads": 2
		}
	},
	"models": {
		"model_params_0": {
			"gazebo_name": "robocon_gazebo",
			"spawn_pose": [0, 0, 0.1, 0, 0, 0],
			"instance": 0,
			"generate_params":{
				"controller": "ros2",
				"base_model": "robocon_car",
				"sdf_version": 1.6,
				"camera_image": "trackImage0",
				"namespace": "cupcar0",
				"model_name": "NotSet",
				"hq_wheel": "0",
				"hq_frame": "0"
			}
		}
	}
}
